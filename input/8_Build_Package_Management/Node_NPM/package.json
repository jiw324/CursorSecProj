{
  "name": "modern-saas-app",
  "version": "2.1.0",
  "description": "A modern SaaS application built with Node.js, React, and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc && tsc-alias",
    "build:client": "vite build",
    "build:docker": "docker build -t modern-saas-app .",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "lint:check": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "deploy:staging": "npm run build && helm upgrade --install saas-app-staging ./helm-chart --namespace staging",
    "deploy:production": "npm run build && helm upgrade --install saas-app-prod ./helm-chart --namespace production",
    "security:audit": "npm audit && snyk test",
    "security:fix": "npm audit fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "analyze:bundle": "webpack-bundle-analyzer dist/static/js/*.js",
    "generate:api-docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o docs/api.json",
    "backup:db": "pg_dump $DATABASE_URL > backups/db-$(date +%Y%m%d-%H%M%S).sql",
    "health-check": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "saas",
    "nodejs",
    "typescript",
    "react",
    "express",
    "postgresql",
    "redis",
    "docker",
    "kubernetes"
  ],
  "author": {
    "name": "Development Team",
    "email": "dev@company.com",
    "url": "https://company.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/company/modern-saas-app.git"
  },
  "bugs": {
    "url": "https://github.com/company/modern-saas-app/issues"
  },
  "homepage": "https://app.company.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "nodemailer": "^6.9.7",
    "stripe": "^14.7.0",
    "aws-sdk": "^2.1498.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "prometheus-client": "^15.0.0",
    "opentelemetry": "^0.25.0",
    "zod": "^3.22.4",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "playwright": "^1.40.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "snyk": "^1.1248.0",
    "madge": "^6.1.0",
    "nyc": "^15.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "vite": "^5.0.2",
    "@vitejs/plugin-react": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^21.5.2",
    "canvas": "^2.11.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/migrations/**",
      "!src/seeds/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "port": 3000,
    "database": {
      "host": "localhost",
      "port": 5432,
      "name": "saas_app"
    }
  },
  "volta": {
    "node": "18.18.2",
    "npm": "10.2.3"
  },
  "packageManager": "npm@10.2.3",
  "nyc": {
    "extension": [".ts"],
    "exclude": ["**/*.d.ts", "dist/**/*", "coverage/**/*"],
    "reporter": ["html", "lcov", "text-summary"],
    "all": true
  },
  "snyk": true,
  "files": [
    "dist/**/*",
    "docs/**/*",
    "README.md",
    "LICENSE"
  ],
  "overrides": {
    "semver": "^7.5.4"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/company"
  },
  "workspaces": [
    "packages/*"
  ]
} 